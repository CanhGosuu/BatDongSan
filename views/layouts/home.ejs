    data-parallax="scroll"
    data-image-src="images/properties.jpg"
    data-speed="0.8"
  ></div>
  <div class="home_container">
    <div class="container">
      <div class="row">
        <div class="col">
          <div
            class="home_content d-flex flex-row align-items-end justify-content-start"
          >
            <div class="home_title"><%- locals.title -%></div>
            <div class="breadcrumbs ml-auto">
              <ul>
                <li><%- locals.title -%></li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
